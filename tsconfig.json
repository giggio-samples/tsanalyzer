{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist",
    "lib": [
      "es2018"
    ],
    "module": "commonjs",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    // "noUnusedParameters": true,
    // "noUnusedLocals": true,
    "rootDir": "./",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es2018"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
  ]
}
